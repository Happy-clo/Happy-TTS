{
  "name": "openai-tts-gradio",
  "version": "1.0.0",
  "description": "OpenAI TTS with Gradio interface",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "ts-node-dev --respawn --transpile-only src/app.ts",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "tsc",
    "build": "npm run build:backend && npm run build:frontend",
    "prod": "npm run build && npm start",
    "test": "jest"
  },
  "keywords": [
    "openai",
    "tts",
    "gradio"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "openai": "^4.0.0",
    "openai-tts-gradio": "file:",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "concurrently": "^8.0.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
